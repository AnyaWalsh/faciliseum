{
  "name": "faciliseum",
  "version": "1.0.0",
  "description": "Compiles GraphQL tagged template strings using graphql-tag in TypeScript files.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnyaWalsh/faciliseum.git"
  },
  "keywords": [
    "consequatur",
    "nesciunt",
    "saepe",
    "unde"
  ],
  "author": "OrrinBode",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/glob": "^5.0.35",
    "@types/jest": "^22.1.3",
    "@types/node": "^9.4.6",
    "debug": "^3.1.0",
    "glob": "^7.1.2",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.0",
    "typescript": "^2.7.2"
  },
  "peerDependencies": {
    "graphql": "^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0",
    "graphql-tag": "^2.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}